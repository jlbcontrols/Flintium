{
  "name": "VariableOwner",
  "parameters": {
    "plcName": {
      "bindType": "parameter",
      "binding": "{plcName}"
    },
    "opcPrefix": {
      "bindType": "parameter",
      "binding": "{opcPrefix}"
    },
    "opcServName": {
      "bindType": "parameter",
      "binding": "{opcServName}"
    },
    "plcTag": {
      "bindType": "parameter",
      "binding": "{plcTag}"
    },
    "bitNumber": "0"
  },
  "tagType": "UdtType",
  "tags": [
    {
      "name": "X",
      "tagType": "Folder",
      "tags": [
        {
          "opcItemPath": {
            "bindType": "parameter",
            "binding": "{opcPrefix}[{plcName}]{plcTag}.Cfg_ExtKeep.{bitNumber}"
          },
          "tagGroup": {
            "bindType": "parameter",
            "binding": "Flintium Leased-Slow"
          },
          "opcServer": {
            "bindType": "parameter",
            "binding": "{opcServName}"
          },
          "valueSource": "opc",
          "dataType": "Boolean",
          "name": "Keep",
          "value": false,
          "tagType": "AtomicTag"
        },
        {
          "opcItemPath": {
            "bindType": "parameter",
            "binding": "{opcPrefix}[{plcName}]{plcTag}.Wrk_OperHas{bitPatternForHas}{bitNumber}"
          },
          "opcServer": {
            "bindType": "parameter",
            "binding": "{opcServName}"
          },
          "valueSource": "expr",
          "accessRights": "Read_Only",
          "expression": "//Use CmdSrc status tags in parent device, and Keep settings, to determine if Ext has ownership of the variable\r\n\r\n( \r\n{[.]../../../CmdSrc/Status/Ext} \u0026\u0026 !{[.]../O/Keep} \u0026\u0026 !{[.]../P/Keep}\r\n)\r\n\r\n||\r\n\r\n(\r\n({[.]../../../CmdSrc/Status/Oper} || {[.]../../../CmdSrc/Status/Prog}) \u0026\u0026 {[.]Keep}\r\n)",
          "dataType": "Boolean",
          "name": "Has",
          "value": false,
          "tagType": "AtomicTag"
        }
      ]
    },
    {
      "name": "O",
      "tagType": "Folder",
      "tags": [
        {
          "opcItemPath": {
            "bindType": "parameter",
            "binding": "{opcPrefix}[{plcName}]{plcTag}.Wrk_OperHas{bitPatternForHas}{bitNumber}"
          },
          "opcServer": {
            "bindType": "parameter",
            "binding": "{opcServName}"
          },
          "valueSource": "expr",
          "accessRights": "Read_Only",
          "expression": "//Use CmdSrc status tags in parent device, and Keep settings, to determine if Oper has ownership of the variable\r\n\r\n( \r\n{[.]../../../CmdSrc/Status/Oper} \u0026\u0026 !{[.]../P/Keep} \u0026\u0026 !{[.]../X/Keep}\r\n)\r\n\r\n||\r\n\r\n{[.]../../../CmdSrc/Status/Maint}\r\n\r\n||\r\n\r\n(\r\n({[.]../../../CmdSrc/Status/Prog} || {[.]../../../CmdSrc/Status/Ext}) \u0026\u0026 {[.]Keep}\r\n)",
          "dataType": "Boolean",
          "name": "Has",
          "value": false,
          "tagType": "AtomicTag"
        },
        {
          "opcItemPath": {
            "bindType": "parameter",
            "binding": "{opcPrefix}[{plcName}]{plcTag}.Cfg_OperKeep.{bitNumber}"
          },
          "tagGroup": {
            "bindType": "parameter",
            "binding": "Flintium Leased-Slow"
          },
          "opcServer": {
            "bindType": "parameter",
            "binding": "{opcServName}"
          },
          "valueSource": "opc",
          "dataType": "Boolean",
          "name": "Keep",
          "value": false,
          "tagType": "AtomicTag"
        }
      ]
    },
    {
      "name": "P",
      "tagType": "Folder",
      "tags": [
        {
          "opcItemPath": {
            "bindType": "parameter",
            "binding": "{opcPrefix}[{plcName}]{plcTag}.Wrk_OperHas{bitPatternForHas}{bitNumber}"
          },
          "opcServer": {
            "bindType": "parameter",
            "binding": "{opcServName}"
          },
          "valueSource": "expr",
          "accessRights": "Read_Only",
          "expression": "//Use CmdSrc status tags in parent device, and Keep settings, to determine if Prog has ownership of the variable\r\n\r\n( \r\n{[.]../../../CmdSrc/Status/Prog} \u0026\u0026 !{[.]../O/Keep} \u0026\u0026 !{[.]../X/Keep}\r\n)\r\n\r\n||\r\n\r\n(\r\n({[.]../../../CmdSrc/Status/Oper} || {[.]../../../CmdSrc/Status/Ext}) \u0026\u0026 {[.]Keep}\r\n)",
          "dataType": "Boolean",
          "name": "Has",
          "value": false,
          "tagType": "AtomicTag"
        },
        {
          "opcItemPath": {
            "bindType": "parameter",
            "binding": "{opcPrefix}[{plcName}]{plcTag}.Cfg_ProgKeep.{bitNumber}"
          },
          "tagGroup": {
            "bindType": "parameter",
            "binding": "Flintium Leased-Slow"
          },
          "opcServer": {
            "bindType": "parameter",
            "binding": "{opcServName}"
          },
          "valueSource": "opc",
          "dataType": "Boolean",
          "name": "Keep",
          "value": false,
          "tagType": "AtomicTag"
        }
      ]
    }
  ]
}